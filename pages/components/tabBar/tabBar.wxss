.tabBar{
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  position: fixed;
  bottom: 0;
  z-index: 1;
  background-color: #FFFFFF;
}
.tabBar_item{
  width: 25%;
  height: 100rpx;
}
.item_text{
  width: 100%;
  height: 100rpx;  
